{
  "name": "annekesinnema",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build-scss",
    "build": "npm run build-storybook && npm run build-scss",
    "build-scss": "sass --watch src/scss/index.scss grav/user/themes/annekesinnema/css/styles.css",
    "dev-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "convert-scss": "sass src/scss/index.scss dist/styles.css",
    "convert-scss-watch": "sass --watch src/scss/index.scss .storybook/styles.css",
    "storybook": "npm run convert-scss && npm run dev-storybook"
  },
  "author": "Anneke Sinnema",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/html": "^5.2.1",
    "parcel-bundler": "^1.12.3",
    "posthtml": "^0.11.6",
    "posthtml-expressions": "^1.1.1",
    "posthtml-include": "^1.2.0",
    "posthtml-md": "^1.1.0",
    "sass": "^1.22.12",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@storybook/addon-a11y": "^5.2.1",
    "babel-loader": "^8.0.6"
  }
